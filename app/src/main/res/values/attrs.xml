<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SquareImageButton">
        <attr name="matchDimension" format="enum">
            <enum name="width" value="0" />
            <enum name="height" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="Header">
        <attr name="btnLeft" format="string" />
        <attr name="btnRight" format="string" />
        <attr name="leftIcon" format="integer" />
        <attr name="rightIcon" format="integer" />
        <attr name="headerTitle" format="string" />
    </declare-styleable>
    <declare-styleable name="Fields">
        <attr name="label" format="string" />
        <attr name="mainTitle" format="string" />
        <attr name="subTitle" format="string" />
        <attr name="label_weight" format="float" />
        <attr name="value_weight" format="float" />
        <attr name="chevronIcon" format="integer" />
        <attr name="emptyText" format="string"/>
    </declare-styleable>
</resources>